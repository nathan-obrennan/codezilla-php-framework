# Ignore src/ as it contains the initial database
# configurations necessary for creation of the system
# and we don't want those to get out without a valid license
# key for purchasers!
system/_install/src/*

# Ignore the modules directory as this is where
# the application should reside, except the welcome module
# which is the default (example) module
modules/*
!modules/welcome/

# Ignore the plugins directory so we don't over write
# custom plugins
plugins/*
!plugins/index.php

# ignore plugins and modules in the vendors folder
common/vendors/*
!common/vendors/index.php
common/themes/admin/*
!common/themes/admin/index.php

